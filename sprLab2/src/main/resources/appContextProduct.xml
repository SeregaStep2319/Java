<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:context="http://www.springframework.org/schema/context"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="TeaLiptonBlack" class="Element.Tea">
        <constructor-arg value="Lipton"/>
        <constructor-arg value="false"/>
        <constructor-arg value="100"/>
    </bean>
    
    <bean id="TeaLiptonGreen" class="Element.Tea">
        <constructor-arg value="Lipton"/>
        <constructor-arg value="true"/>
        <constructor-arg value="100"/>
    </bean>
    
    <bean id="TeaGreenfieldBlack" class="Element.Tea">
        <constructor-arg value="Greenfield"/>
        <constructor-arg value="false"/>
        <constructor-arg value="120"/>
    </bean>
    
    <bean id="TeaGreenfieldGreen" class="Element.Tea">
        <constructor-arg value="Greenfield"/>
        <constructor-arg value="true"/>
        <constructor-arg value="120"/>
    </bean>
    
    <bean id="CoffeeJacobsBlack" class="Element.Coffee">
        <constructor-arg value="Jacobs"/>
        <constructor-arg value="false"/>
        <constructor-arg value="80"/>
    </bean>
    
    <bean id="CoffeeJacobsMilk" class="Element.Coffee">
        <constructor-arg value="Jacobs"/>
        <constructor-arg value="true"/>
        <constructor-arg value="80"/>
    </bean>
    
    <bean id="CoffeeJardinBlack" class="Element.Coffee">
        <constructor-arg value="Jardin"/>
        <constructor-arg value="false"/>
        <constructor-arg value="70"/>
    </bean>
    
    <bean id="CoffeeJardinMilk" class="Element.Coffee">
        <constructor-arg value="Jardin"/>
        <constructor-arg value="true"/>
        <constructor-arg value="70"/>
    </bean>
    
</beans>